<form class="form-horizontal">
  <div class="control-group">
      <label class="control-label"><i class="fa fa-camera"></i></label>
      <div class="controls">
          <input type="text" class="input-block-level" data-bind="value: settings.plugins.jobscheduler.url">
      </div>
  </div>
  <div class="control-group col-3">
      <label class="control-label"><i class="fa fa-sun-o"></i></label>
      <div class="controls">
          <input type="text" class="input-block-level" data-bind="value: settings.plugins.jobscheduler.day">
      </div>
  </div>
  <div class="control-group col-3">
      <label class="control-label"><i class="fa fa-moon-o"></i></label>
      <div class="controls">
          <input type="text" class="input-block-level" data-bind="value: settings.plugins.jobscheduler.night">
      </div>
  </div>


  <div class="form-row mb-4">
      <div class="col">
          <!-- First name -->
          <input type="text" id="defaultRegisterFormFirstName" class="form-control" placeholder="First name">
      </div>
      <div class="col">
          <!-- Last name -->
          <input type="text" id="defaultRegisterFormLastName" class="form-control" placeholder="Last name">
      </div>
  </div>

</form>
